{
  "compilerOptions": {
    "downlevelIteration": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitUseStrict": true,
    "jsx": "preserve"
  },
  "exclude": [
    "obj",
    "bin",
    "node_modules",
    "wwwroot"
  ]
}
